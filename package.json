{
  "name": "raspar",
  "description": "A simple to use Promise-based web scraper with local caching.",
  "version": "1.1.2",
  "main": "index.js",
  "engines": {
    "node": ">=4.0"
  },
  "license": "MIT",
  "dependencies": {
    "cache-manager": "2.4.0",
    "es6-promisify": "5.0.0",
    "mkdirp": "0.5.1",
    "request": "2.81.0"
  },
  "devDependencies": {
    "@neogeek/eslint-config-standards": "^2.0.0",
    "chai": "^4.1.1",
    "codecov": "^2.3.0",
    "doxdox": "^2.0.3",
    "eslint": "^4.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^4.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run clean && mocha test/specs/",
    "test:docs": "doxdox 'lib/**/*.js' -l markdown | diff DOCUMENTATION.md -",
    "lint": "eslint .",
    "coverage": "istanbul cover _mocha test/specs/ && codecov",
    "clean": "rm -rf temp/",
    "docs": "doxdox 'lib/**/*.js' -l markdown -o DOCUMENTATION.md"
  },
  "keywords": [
    "request",
    "cache",
    "promise"
  ],
  "authors": [
    {
      "name": "Scott Doxey",
      "email": "hello@scottdoxey.com",
      "homepage": "http://scottdoxey.com/"
    }
  ],
  "homepage": "https://github.com/neogeek/raspar",
  "repository": {
    "type": "git",
    "url": "git://github.com/neogeek/raspar.git"
  }
}
